pipeline{
        agent any
        stages {
                stage('compile') {
                        steps {
                                echo 'compiling ..'
                                git url: 'https://github.com/venkatsiva369/cicd_pipeline_tutorial'
                                sh script: '/opt/apache-maven-3.9.2/bin/mvn compile'
                                }
                        }
              }

    }
